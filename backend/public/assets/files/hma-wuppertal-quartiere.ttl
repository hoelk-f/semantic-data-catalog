@prefix cm: <http://plasma.uni-wuppertal.de/cm#> .
@prefix sm: <http://plasma.uni-wuppertal.de/sm/> .
@prefix vc: <http://www.tmdt.uni-wuppertal.de/vcslam#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

sm:herbringhausen a vc:district ;
    cm:originalLabel "Herbringhausen" ;
    vc:has sm:sensor_herbringhausen .

sm:lat_herbringhausen a vc:latitude ;
    cm:originalLabel "lat" ;
    cm:value 5.123033e+01 .

sm:lng_herbringhausen a vc:longitude ;
    cm:originalLabel "lng" ;
    cm:value 7.2738e+00 .

sm:sensor_herbringhausen a vc:sensor ;
    vc:has sm:lat_herbringhausen,
        sm:lng_herbringhausen,
        sm:status_herbringhausen,
        sm:temp_herbringhausen .

sm:status_herbringhausen a vc:status ;
    cm:originalLabel "activated" ;
    cm:value true .

sm:temp_herbringhausen a vc:temperature ;
    cm:originalLabel "temp" ;
    cm:value 0 .