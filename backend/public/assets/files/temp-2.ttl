@prefix dc:     <http://purl.org/dc/elements/1.1/> .
@prefix flo:    <http://flight-ontology.com/> .
@prefix local:  <https://local.ontology#> .
@prefix monto:  <http://machine-ontology.com/> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix plasma: <http://plasma.uni-wuppertal.de/schema#> .
@prefix plcm:   <http://plasma.uni-wuppertal.de/cm#> .
@prefix plsm:   <http://plasma.uni-wuppertal.de/sm/> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sosa:   <http://www.w3.org/ns/sosa/> .
@prefix vcslam: <http://www.tmdt.uni-wuppertal.de/vcslam#> .
@prefix ws:     <http://workshop-ontology.com#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .

plsm:fdbe0-location_1
        rdf:type            vcslam:location ;
        plcm:originalLabel  "location" ;
        vcslam:has          plsm:fdbe0-district_1 .

plsm:fdbe0-longitude_1
        rdf:type            vcslam:longitude ;
        plcm:originalLabel  "longitude" .

plsm:fdbe0-t_1  rdf:type    vcslam:temperature ;
        plcm:originalLabel  "t" .

plsm:fdbe0-district_1
        rdf:type            vcslam:district ;
        plcm:originalLabel  "district" .

plsm:fdbe0-activated_1
        rdf:type            vcslam:status ;
        plcm:originalLabel  "activated" .

plsm:fdbe0-root_1  rdf:type  vcslam:sensor ;
        plcm:originalLabel  "ROOT" ;
        vcslam:has          plsm:fdbe0-activated_1 ;
        vcslam:locatedAt    [ rdf:type    vcslam:geoposition ;
                              vcslam:has  plsm:fdbe0-latitude_1 , plsm:fdbe0-longitude_1
                            ] ;
        vcslam:locatedIn    plsm:fdbe0-location_1 , plsm:fdbe0-district_1 ;
        vcslam:measures     plsm:fdbe0-t_1 .

plsm:fdbe0-latitude_1
        rdf:type            vcslam:latitude ;
        plcm:originalLabel  "latitude" .
