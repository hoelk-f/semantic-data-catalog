    vcslam:id "et_2cb463ee-105a-4184-87bf-f23b4e4d1f2e" ;
    rdfs:comment "The World Geodetic System (WGS) is a standard for use in cartography, geodesy, and satellite navigation including GPS." .

vcslam:approximate_location a plasma:description ;
    rdfs:label "approximate location" ;
