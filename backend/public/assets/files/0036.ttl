        vcslam:neighbourhood_name ;
    plasma:has vcslam:human_address ;
    plasma:locatedAt vcslam:geoposition ;
    vcslam:id "et_2935474e-21b5-4107-995a-88e8fe0d086d" ;
    rdfs:comment "a particular place or position." .

vcslam:event_close_time a plasma:datetime,
        plasma:time ;
    rdfs:label "event close time",
        "event_close_time" ;
    plasma:isAfter vcslam:dispatch_time ;
    plasma:isPartOf vcslam:event_close_time ;
    vcslam:id "et_6c1e0462-9836-4e80-bb65-63135b11a57f",
        "et_bf041c31-231e-49bf-8e33-35e2d8568a93" ;
    vcslam:originalLabel "event_close_time" ;
    rdfs:comment "Time when the last fire unit leaves the place of incident",
        "a point of time as measured in hours and minutes past midnight or noon" .

vcslam:dispatch_time a plasma:datetime,
        plasma:time ;
    rdfs:label "dispatch time",
        "dispatch_time" ;
    plasma:has vcslam:day_of_week,
        vcslam:dispatch_month_name ;
    plasma:isBefore vcslam:event_close_time ;
    plasma:isPartOf vcslam:dispatch_time ;
    vcslam:id "et_336ed367-0615-4c8f-88b2-f88b72cb0b91",
        "et_4cc5e026-6362-4867-91d7-fb3d34038e3b" ;
    vcslam:originalLabel "dispatch_time" ;
    rdfs:comment "a point of time as measured in hours and minutes past midnight or noon",
        "time when the first respond unit is dispatched" .

