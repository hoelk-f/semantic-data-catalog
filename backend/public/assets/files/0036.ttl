    plasma:isNear vcslam:location ;
    vcslam:id "et_d627175d-2968-4c8a-851b-7b78188526c6" ;
    vcslam:originalLabel "approximate_location" ;
    rdfs:comment "specifies nearby intersecting street names" .

vcslam:fire_response a plasma:incident ;
    rdfs:label "fire response" ;
