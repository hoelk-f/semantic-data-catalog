vcslam:occurrence_month a plasma:month ;
    rdfs:label "Occurrence Month" ;
    vcslam:id "et_a10e1efd-eb3f-459f-b07f-1e4fb1ebac89" ;
    vcslam:originalLabel "Occurrence_Month" ;
    rdfs:comment "Occurrence Month" .

vcslam:occurrence_time a plasma:time ;
    rdfs:label "Occurrence Time" ;
    vcslam:id "et_74c0b52e-7ea1-478a-80d3-24fb93ba576b" ;
