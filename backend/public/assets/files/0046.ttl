    rdfs:label "event unique id" ;
    vcslam:id "et_5cd8fc5d-36d2-47ce-97f5-16691a4502ef" ;
    vcslam:originalLabel "event_unique_id" ;
    rdfs:comment "Event Occurrence Identifier" .

vcslam:geoposition a plasma:geoposition ;
