    vcslam:id "et_c5168f94-10d9-4aa3-a8e6-ee53b0c2dff7" ;
    vcslam:originalLabel "Bike_Make" ;
    rdfs:comment "a company that produces something" .

vcslam:bike_speed a plasma:kmh ;
    rdfs:label "Bike_Speed" ;
    vcslam:id "et_62698e03-0ee1-4540-86c3-0519a38da597" ;
