    plasma:occuredOn vcslam:datetime ;
    vcslam:id "et_3378d647-4dea-4386-b39e-b107e9f465c2" ;
    rdfs:comment "the crime of stealing" .

vcslam:type a plasma:geo_type ;
    rdfs:label "type" ;
    vcslam:id "et_59366e8b-3f47-4f29-90cb-a999cda71ee9" ;
    vcslam:originalLabel "type" ;
    rdfs:comment "type of the geometic shape (like point, line or polygon)" .

vcslam:bike a plasma:bike ;
    rdfs:label "bike" ;
    plasma:costs vcslam:cost_of_bike ;
    plasma:has vcslam:bike_colour,
        vcslam:bike_make,
        vcslam:bike_speed,
        vcslam:status ;
    plasma:parkedAt vcslam:location ;
    plasma:specifiedBy vcslam:bike_type ;
    plasma:stolenDuring vcslam:theft ;
    vcslam:id "et_8fe4eff2-7cd2-433c-9a88-4588e650bd49" ;
    rdfs:comment "a bicycle, a vehicle with two wheels" .

vcslam:city a plasma:city ;
    rdfs:label "City" ;
    vcslam:id "et_d5521a1c-6211-475c-849a-d614ff0c0c39" ;
    vcslam:originalLabel "City" ;
    rdfs:comment "a larger town, usually consists of multiple districts" .

vcslam:location a plasma:location ;
    rdfs:label "location" ;
    plasma:definedBy vcslam:geoposition ;
    plasma:has vcslam:location_type,
        vcslam:premise_type,
        vcslam:type ;
    plasma:locatedIn vcslam:city,
        vcslam:neighbourhood ;
    vcslam:id "et_51b20fe1-3fae-4402-9044-85780f47b4fb" ;
    rdfs:comment "a particular place or position." .

vcslam:name a plasma:wgs84 ;
    rdfs:label "name" ;
    vcslam:id "et_a2622819-e3f9-464b-be0a-17e027e3e3a1" ;
    vcslam:originalLabel "name" ;
    rdfs:comment "The World Geodetic System (WGS) is a standard for use in cartography, geodesy, and satellite navigation including GPS." .

vcslam:primary_offence a plasma:offense ;
    rdfs:label "Primary Offence" ;
    plasma:has vcslam:status ;
    plasma:identifiedBy vcslam:event_unique_id ;
    plasma:occuredAt vcslam:location ;
    vcslam:id "et_c65aa764-c4d4-42d3-a6f9-30c7bf9c0f81" ;
    vcslam:originalLabel "Primary_Offence" ;
    rdfs:comment "Offence related to the occurrence" .

vcslam:status a plasma:status ;
    rdfs:label "Status" ;
    vcslam:id "et_7bb9e137-7df5-4ad3-b757-8defdf84507e" ;
