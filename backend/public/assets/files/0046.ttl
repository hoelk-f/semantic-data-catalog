    plasma:definedBy vcslam:name ;
    vcslam:id "et_93095dbb-a9fa-4338-80ee-67c81002a5e5" ;
    vcslam:originalLabel "latitude" ;
    rdfs:comment "the angular distance of a place north or south of the earth's equator, or of the equator of a celestial object, usually expressed in degrees and minutes." .

vcslam:location_type a plasma:type ;
    rdfs:label "Location Type" ;
    vcslam:id "et_f8133316-4b43-4ef8-a6da-5842d2bb47d9" ;
    vcslam:originalLabel "Location_Type" ;
    rdfs:comment "Location Type where event occurred" .

vcslam:longitude a plasma:longitude ;
