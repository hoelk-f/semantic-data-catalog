@prefix plasma: <http://plasma.uni-wuppertal.de/schema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vcslam: <http://plasma.uni-wuppertal.de/vcslam#> .

vcslam:bill a plasma:bill ;
    rdfs:label "bill" ;
    plasma:createdAt vcslam:bill_date ;
    plasma:has vcslam:batch_number,
        vcslam:net ;
    plasma:identifiedBy vcslam:bill_code ;
