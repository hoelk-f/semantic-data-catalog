vcslam:latitude a plasma:latitude ;
    rdfs:label "latitude" ;
    plasma:definedBy vcslam:wgs84 ;
    plasma:sameAs vcslam:street_centroid_lat ;
    vcslam:id "et_ea6a21f9-9243-46ac-b222-6a193bb8b1a5" ;
