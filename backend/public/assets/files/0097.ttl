vcslam:square_foot a plasma:square_foot ;
    rdfs:label "square foot" ;
    vcslam:id "et_3f9c0212-f299-4eb9-9507-1553fd038e4e" ;
    rdfs:comment "a unit of area measurement equal to 0.0929 square meters" .

vcslam:stormwater_gallons_saved a plasma:value ;
    rdfs:label "stormwater gallons saved" ;
    plasma:cause vcslam:stormwater_benefits ;
    plasma:measuredIn vcslam:gallon ;
    vcslam:id "et_18c09439-8473-440b-bf06-c6b7973e2ebe" ;
    vcslam:originalLabel "stormwater_gallons_saved" ;
    rdfs:comment "The total amount of gallons of water saved by a tree as a result of intercepting and storing rainfall in leaves and branches, increasing soil infiltration through root growth and decomposition, reducing soil erosion by diminishing the impact of rain on barren surfaces, and reducing soil moisture thereby increasing the soil's capacity to store rainfall." .

vcslam:street a plasma:street ;
    rdfs:label "street" ;
    vcslam:id "et_def41131-1139-4264-9615-b589aaa362b9" ;
    vcslam:originalLabel "street" ;
    rdfs:comment "a public road in a city, town, or village, typically with houses and buildings on one or both sides." .

vcslam:therms_saved a plasma:value ;
    rdfs:label "therms saved" ;
    vcslam:id "et_9cb2c392-ed04-441f-9060-6f410d783aa8" ;
    vcslam:originalLabel "therms_saved" ;
    rdfs:comment "the amount of therms of heat energy saved by reducing wind speeds." .

vcslam:total_yearly_eco_benefits a plasma:benefit ;
    rdfs:label "total yearly eco benefits" ;
    plasma:consistsOf vcslam:air_quality_benefits,
        vcslam:energy_benefits,
        vcslam:greenhouse_co2_benefits,
        vcslam:property_benefits,
        vcslam:stormwater_benefits ;
    plasma:measuredIn vcslam:dollar ;
    vcslam:id "et_c4d63073-7ccf-47b9-921d-c4103bb83166" ;
    vcslam:originalLabel "total_yearly_eco_benefits" ;
    rdfs:comment "The total dollar amount of benefits that a tree provides. This is a combination of greenhouse gas benefits, water benefits, energy benefits, air quality benefits, and property benefits." .

vcslam:tree a plasma:tree ;
    rdfs:label "tree" ;
    plasma:has vcslam:air_quality_benefits,
        vcslam:common_name,
        vcslam:dbh,
        vcslam:energy_benefits,
        vcslam:greenhouse_co2_benefits,
        vcslam:leaf,
        vcslam:property_benefits,
        vcslam:stormwater_benefits,
        vcslam:total_yearly_eco_benefits ;
    plasma:isA vcslam:botanical_name,
        vcslam:plant ;
    plasma:locatedAt vcslam:location,
        vcslam:side ;
    plasma:locatedIn vcslam:park_name ;
    plasma:responsibleFor vcslam:avoided_co2,
        vcslam:kwh_saved,
        vcslam:pollutants_saved_in_lbs,
        vcslam:stormwater_gallons_saved,
        vcslam:therms_saved ;
    vcslam:id "et_d4c1b2fc-8275-4f41-b024-f4066d55440e" ;
    rdfs:comment """a plant that has a wooden trunk and bears lateral branches
""" .

vcslam:energy_benefits a plasma:benefit ;
    rdfs:label "energy benefits" ;
    plasma:measuredIn vcslam:dollar ;
    vcslam:id "et_5e3710bc-354c-4f1e-8e10-9592e3b1ebce" ;
    vcslam:originalLabel "energy_benefits" ;
    rdfs:comment "The total dollar amount of benefits that a tree provides by modifying climate and conserving building energy use." .

vcslam:greenhouse_co2_benefits a plasma:benefit ;
    rdfs:label "greenhouse co2 benefits" ;
    plasma:measuredIn vcslam:dollar ;
    vcslam:id "et_0c952e0b-31f9-4ece-91b5-7f219872b4fc" ;
    vcslam:originalLabel "greenhouse_co2_benefits" ;
    rdfs:comment "The total dollar amount of benefits that a tree provides by reducing atmospheric greenhouse gases" .

vcslam:latitude a plasma:latitude ;
    rdfs:label "latitude" ;
    plasma:definedBy vcslam:wgs84 ;
    plasma:sameAs vcslam:latitude ;
    vcslam:id "et_15c615f2-0628-405e-9366-9582854299cc",
        "et_91b7e4f3-9a32-48a6-9fe1-5d95e3dbfd17" ;
    vcslam:originalLabel "latitude" ;
    rdfs:comment "the angular distance of a place north or south of the earth's equator, or of the equator of a celestial object, usually expressed in degrees and minutes." .

vcslam:location a plasma:location ;
    rdfs:label "location" ;
    plasma:definedBy vcslam:geoposition ;
    plasma:has vcslam:address ;
    plasma:locatedIn vcslam:council_district,
        vcslam:park_name ;
    vcslam:id "et_fe44848b-4fc2-4ed0-a6db-33fefe2ce91f" ;
    rdfs:comment "a particular place or position." .

vcslam:longitude a plasma:longitude ;
    rdfs:label "longitude" ;
    plasma:definedBy vcslam:wgs84 ;
    plasma:sameAs vcslam:longitude ;
    vcslam:id "et_097e831b-8e59-4911-89c1-e4ac44095579",
        "et_7ca70d20-10eb-4050-8eb9-e601df56c7ed" ;
    vcslam:originalLabel "longitude" ;
    rdfs:comment "the position to the east or west of an imaginary circle around the earth that goes through the North Pole, the South Pole, and Greenwich, England" .

vcslam:property_benefits a plasma:benefit ;
    rdfs:label "property benefits" ;
    plasma:measuredIn vcslam:dollar ;
    vcslam:id "et_8fb5ef12-f894-4d28-9328-efaa17b489a2" ;
    vcslam:originalLabel "property_benefits" ;
    rdfs:comment "The total dollar amount of increased property value through the increased curb appeal of having a tree in front of a property. Research has shown that buyers are willing to pay more for homes with ample trees." .

vcslam:wgs84 a plasma:wgs84 ;
    rdfs:label "WGS84" ;
    vcslam:id "et_2a8ea5f2-80a8-4363-9b76-6592a4fe8632" ;
    rdfs:comment "The World Geodetic System (WGS) is a standard for use in cartography, geodesy, and satellite navigation including GPS." .

vcslam:air_quality_benefits a plasma:benefit ;
    rdfs:label "air quality benefits" ;
    plasma:concerns vcslam:air_quality ;
    plasma:measuredIn vcslam:dollar ;
    vcslam:id "et_e6650dfe-f03d-4893-9487-0a691a96e27f" ;
    vcslam:originalLabel "air_quality_benefits" ;
    rdfs:comment "The total dollar amount of benefits that a tree provides by increasing air quality through absorption of gaseous pollutants, intercepting particulates, releasing oxygen, transpiring water, shading surfaces, lowering air temperatures and reducing ozone levels." .

vcslam:council_district a plasma:district ;
