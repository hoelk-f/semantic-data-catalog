    vcslam:id "et_63cac39e-d484-4f6d-a9f3-538ca723036a" ;
    rdfs:comment "a unit of one thousand watts of electrical power." .

vcslam:kwh_saved a plasma:value ;
    rdfs:label "kwh saved" ;
    plasma:measuredIn vcslam:kilowatt ;
    vcslam:id "et_f776e8ab-9537-4f1e-a1b1-f6133485eb96" ;
    vcslam:originalLabel "kwh_saved" ;
    rdfs:comment "The amount of kilowatt hours of energy saved by shading and transpiration (converting liquid water to water vapor which cools the environment)." .

vcslam:leaf a plasma:leaf ;
    rdfs:label "leaf" ;
