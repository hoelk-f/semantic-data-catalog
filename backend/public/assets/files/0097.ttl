    vcslam:id "et_eee19158-d688-423a-8a73-1d463d1d294a" ;
    vcslam:originalLabel "pollutants_saved_in_lbs" ;
    rdfs:comment "The total amount of pollutants saved (in pounds) by a tree through absorbing gaseous pollutants, intercepting particulates, and reducing ozone levels." .

vcslam:side a plasma:position ;
    rdfs:label "side" ;
    plasma:locatedIn vcslam:park_name ;
