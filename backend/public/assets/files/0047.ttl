        vcslam:bld_typo,
        vcslam:dev_name,
        vcslam:flr_abv_gr,
        vcslam:mrkt_unit ;
    plasma:identifiedBy vcslam:bld_id,
        vcslam:dev_id,
        vcslam:objectid ;
