    plasma:validFor vcslam:washroom ;
    vcslam:id "et_5624bbdc-58cf-4d39-80d2-965b2ce7f2b8" ;
    vcslam:originalLabel "note" ;
    rdfs:comment "facts provided or learned " .

vcslam:address a plasma:address ;
