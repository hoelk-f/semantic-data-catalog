    vcslam:id "et_a8fc96a6-5bf9-43c9-a782-ec7f180c4f44" ;
    vcslam:originalLabel "summer_hours" ;
    rdfs:comment "the time when access to a facility or service is possible" .

vcslam:type a plasma:type ;
    rdfs:label "type" ;
    vcslam:id "et_771d14f4-291b-472b-836b-9f6139009cc0" ;
    vcslam:originalLabel "type" ;
