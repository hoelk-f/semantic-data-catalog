    rdfs:label "Washroom" ;
    plasma:has vcslam:name,
        vcslam:summer_hours,
        vcslam:type,
        vcslam:wheel_access,
        vcslam:winter_hours ;
    plasma:identifiedBy vcslam:primaryind ;
    plasma:locatedAt vcslam:location ;
    vcslam:id "et_a8b9bd78-e9f7-49ad-94c1-5a157194af85" ;
    rdfs:comment "a room with washing and toilet facilities." .

vcslam:wgs84 a plasma:wgs84 ;
    rdfs:label "WGS84" ;
    vcslam:id "et_e96e5eeb-4118-42d4-b882-e8076fa66633" ;
    rdfs:comment "The World Geodetic System (WGS) is a standard for use in cartography, geodesy, and satellite navigation including GPS." .

vcslam:location a plasma:description,
        plasma:location ;
    rdfs:label "location" ;
    plasma:definedBy vcslam:geoposition ;
    plasma:describes vcslam:location ;
    plasma:has vcslam:address,
        vcslam:geo_type,
        vcslam:location ;
    plasma:locatedAt vcslam:geo_local_area ;
    vcslam:id "et_13f3bbc7-897a-42b6-8c41-5df27e235d96",
        "et_afd8a046-539a-400d-bafb-7d8c25e68817" ;
    vcslam:originalLabel "location" ;
    rdfs:comment "a particular place or position.",
        "a spoken or written account of a person, object, or event." .

