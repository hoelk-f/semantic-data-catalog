    vcslam:id "et_31d102a5-187f-49d6-bbcc-47ca2c4c253d" ;
    vcslam:originalLabel "primaryind" ;
    rdfs:comment "a name, series of numbers, etc. that is used in a system to represent someone or something" .

vcslam:summer a plasma:summer ;
    rdfs:label "summer" ;
    vcslam:id "et_4c9e8411-43b4-46ac-82ec-3ca30c766d4e" ;
