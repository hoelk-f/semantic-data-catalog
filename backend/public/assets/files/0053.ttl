vcslam:wheel_access a plasma:wheel_access ;
    rdfs:label "wheel_access" ;
    vcslam:id "et_cc9d0aaa-233b-4236-b09b-dbf12a316d6d" ;
    vcslam:originalLabel "wheel_access" ;
    rdfs:comment "a method helping someone with wheel chair to enter or leave a facility" .

vcslam:winter a plasma:winter ;
    rdfs:label "winter" ;
    vcslam:id "et_42b08152-c13e-4740-b5f2-ea4220f689d6" ;
    rdfs:comment "the coldest season of the year" .

vcslam:winter_hours a plasma:opening_hours ;
    rdfs:label "winter_hours" ;
    plasma:validFor vcslam:winter ;
    vcslam:id "et_368cff8d-4a36-458a-a955-02715dca2f47" ;
    vcslam:originalLabel "winter_hours" ;
    rdfs:comment "the time when access to a facility or service is possible" .

